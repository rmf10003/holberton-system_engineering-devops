#!/usr/bin/env bash
# loop to print num 1-20 with conditional formatting
num=1
while [ $num -lt 21 ]
do
    echo $num
    if [ $num -eq 4 ]
    then
	echo bad luck from China
    elif [ $num -eq 9 ]
    then
	echo bad luck from Japan
    elif [ $num -eq 17 ]
    then
	echo bad luck from Italy
    fi
    num=$((count + 1))
done
